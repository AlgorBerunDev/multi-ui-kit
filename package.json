  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/multi-ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/multi-ui-kit/issues"
  },
  "homepage": "https://github.com/yourusername/multi-ui-kit#readme",
  "dependencies": {
    "@mui/material": "^6.1.5",
    "antd": "^5.21.6",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
